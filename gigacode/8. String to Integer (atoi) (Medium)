def myAtoi(self, s: str) -> int:
    s = s.strip()
    if not s:
        return 0

    is_negative = False
    if s[0] in ('-', '+'):
        is_negative = s[0] == '-'
        s = s[1:]

    result = 0
    for char in s:
        if not char.isdigit():
            break
        result = result * 10 + int(char)

    result = result if not is_negative else -result
    if result < -2**31:
        return -2**31
    elif result > 2**31 - 1:
        return 2**31 - 1
    else:
        return result
