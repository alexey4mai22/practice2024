def letterCombinations(self, digits: str) -> List[str]:
    if not digits:
        return []

    mapping = {'2': 'abc', '3': 'def', '4': 'ghi', '5': 'jkl', '6': 'mno', '7': 'pqrs', '8': 'tuv', '9': 'wxyz'}
    result = ['']

    for digit in digits:
        temp = []
        for s in result:
            for c in mapping[digit]:
                temp.append(s + c)
        result = temp

    return result
